              {
                "@type": "Review",
                "@id": "https://runway.com/customer-stories#review-derek",
                "itemReviewed": {
                  "@id": "https://runway.com/customer-stories#software"
                },
                "author": {
                  "@type": "Person",
                  "name": "Derek Ou-Ponticelli",
                  "jobTitle": "CFO",
                  "worksFor": {
                    "@type": "Organization",
                    "name": "AngelList"
                  }
                },
                "reviewBody": "It's about more than just saving time. With Runway, spotting trends is easier\u00e2\u0080\u0094now we can more efficiently tell stakeholders what number a metric's going to be and why.",
                "reviewRating": {
                  "@type": "Rating",
